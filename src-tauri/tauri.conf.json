{
  "productName": "Tasca Do Vereda",
  "version": "1.0.6",
  "identifier": "ao.vereda.gestao.restaurante",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Tasca Do VEREDA - Gest√£o Inteligente",
        "label": "main",
        "width": 1280,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "transparent": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["msi"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "publisher": "Vereda Systems Angola",
    "windows": {
      "wix": {
        "language": "pt-BR"
      }
    }
  },
  "plugins": {
    "sql": {
      "preload": ["sqlite:tasca_vereda.db"]
    }
  }
}
